{
    "manifest_version": 3,
    "name": "Mi Extensión de Tareas",
    "version": "1.0",
    "description": "Una extensión para gestionar tareas con registro de usuarios.",
    "action": {
      "default_popup": "html/register.html",
      "default_icon": {
        "16": "main/icono.png",
        "48": "main/icono.png",
        "128": "main/icono.png"
      }
    },
    "permissions": [
      "storage"
    ]
  }
  